{
  "expo": {
    "scheme": "trucasts",
    "android": {
      "package": "com.omagaowi12.trucasts",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [
            {
              "scheme": "trucasts"
            },
            {
              "scheme": "https",
              "host": "joc-line-frontend.vercel.app",
              "pathPrefix": "/signup"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.omagaowi12.trucasts",
      "associatedDomains": ["applinks:joc-line-frontend.vercel.app"]
    },
    "extra": {
      "eas": {
        "projectId": "93f60fdb-75a4-4074-9f48-7a2effdda765"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-web-browser",
        {
          "experimentalLauncherActivity": false
        }
      ]
    ]
  }
}
